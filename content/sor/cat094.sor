<script>
function mapFunction(name){
	document.body.style.webkitOverflowScrolling = "auto";

	//SET VAR TO CARRIE FILE TO FILL THE IFRAME FROM ONCLICK FUNCTION ON IMAGE MAP
	vidFile = name;
	
	//BUILD GRAY BACKGROUND
	var sldContainer = document.getElementById("mapListingContainer");
	sldContainer.style.display = "block";
	sldContainer.style.cursor = "pointer";
	
	//BUILD CONTAINER FOR IFRAME
	var showContainer = document.getElementById("placeVid");
	showContainer.style.webkitOverflowScrolling = "touch";
	showContainer.style.overflowY ="auto";
	showContainer.style.width = "80%";
	showContainer.style.height = "80%";
	
	//BUILD IFRAME
	sld = document.createElement("iframe");
	sld.setAttribute("src", vidFile );
	sld.setAttribute("id", "mapFile");
	sld.style.width = "100%";
	sld.style.height = "92%";	
	sld.style.backgroundColor = "#ffffff";
	sld.style.border = "none";
	sld.style.overflow = "hidden";
	
	//PLACE THE IFRAME AND ITS TITLE BAR
	$("#placeVid").html('<div class="imgMapTitle">Click outside this window to exit</div> <div class="closeImgMap" id="closit"></div>');
	
	document.getElementById("placeVid").appendChild(sld);
		
	//STOP BROWSER BG FROM SCROLLING WHILE MAP LISTING IS OPENED
	var pbStopScroll = document.body;
	pbStopScroll.style.overflowY = "hidden";

	//CLICK THE BACKGROUND TO REMOVE THE IFRAME
	
	$('#mapListingContainer').on("click", function () {
		sldContainer.style.display = "none";	
		pbStopScroll.style.overflowY = "visible";
	
		//CUT THE FOLLOW CODE THAT REMOVES THE IFRAME AND PASTE IT IN THE PTR AS A FUNCTION OF THE BUY BUTTON
		
		var iframes = document.getElementsByTagName("iframe");
		for (var i = 0; i < iframes.length; i++) {
		  iframes.parentNode.removeChild(iframes);
		  document.body.style.webkitOverflowScrolling = "auto";
		}
	});
}
</script>

