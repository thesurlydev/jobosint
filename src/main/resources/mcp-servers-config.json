{
  "mcpServers": {
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "DEBUG": "true"
      }
    },
    "fetch": {
      "command": "podman",
      "args": ["run", "-i", "--rm", "mcp/fetch"],
      "env": {
        "DEBUG": "true"
      }
    }
  }
}